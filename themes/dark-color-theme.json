{
	"name": "Cereal Dark",
	"semanticHighlighting": true,
	"type": "dark",
	"colors": {
		//editor background
		"editor.background": "#272833",
		"editorBracketMatch.border": "#272833",
		//editor foreground
		"editor.foreground": "#dbd9cc",
		//editor error
		"editorError.foreground": "#ff4281",
		"editorError.background": "#ff0d7e56",
		"list.errorForeground": "#ff428a",
		"minimap.errorHighlight": "#ff4262",
		//editor cursor
		"editor.lineHighlightBackground": "#2f3242",
		"editor.lineHighlightBorder": "#343647",
		"editorBracketMatch.background": "#8f96c98f",
		"editorCursor.foreground": "#ffffff",
		//editor selection wordhighlight
		"editor.wordHighlightBackground": "#b4b3af59",
		"editor.wordHighlightStrongBackground": "#e69d1660",
		//editor selection 
		"editor.selectionBackground": "#88807083",
		"editor.selectionHighlightBackground": "#bfb9f533",
		"editor.inactiveSelectionBackground": "#f0fcc41f",
		"editor.hoverHighlightBackground": "#d7d5e446",
		"editor.findMatchBackground": "#e69d1693",
		"editor.findMatchHighlightBackground": "#e6af1660",
		//editor line number
		"editorLineNumber.foreground": "#60627a",
		"editorLineNumber.activeForeground": "#b9b29f",
		//editor indent guideline
		"editorIndentGuide.background": "#81829638",
		"editorIndentGuide.activeBackground": "#666880f8",
		//editor gutter
		"editorGutter.addedBackground": "#748068",
		"editorGutter.modifiedBackground": "#4e9fb8",
		"editorGutter.deletedBackground": "#bd525b",
		//widget
		"widget.shadow": "#3a3121",
		"editorWidget.border": "#6b4e18",
		"editorWidget.background": "#000000",
		"editorHoverWidget.background": "#000000",
		"editorHoverWidget.border": "#000000",
		//list
		"list.activeSelectionForeground": "#000000",
		"list.hoverBackground": "#39456ec0",
		"list.inactiveSelectionBackground": "#583807",
		"list.inactiveSelectionForeground": "#f5f1e7",
		"list.activeSelectionIconForeground": "#000000",
		"list.focusHighlightForeground": "#000000",
		"list.highlightForeground": "#f0ad31",
		"list.activeSelectionBackground": "#f0ad31",
		//input
		"input.background": "#000000",
		"input.foreground": "#e0dcd0",
		"inputOption.activeBackground": "#f0ad31",
		"inputOption.activeForeground": "#000000",
		//sidebar & activitybar backgrounds
		"sideBar.background": "#1a1d24",
		"activityBar.background": "#1a1d24",
		"sideBarSectionHeader.background": "#1a1d24",
		"panelTitle.activeBorder": "#1a1d24",
		"activityBar.activeBorder": "#1a1d24",
		"tab.inactiveBackground": "#0e1013",
		"editorGroupHeader.tabsBackground": "#0e1013",
		//status bar
		"statusBar.background": "#000000",
		"statusBar.debuggingForeground": "#ffffff",
		"statusBar.debuggingBackground": "#9351df",
		"statusBarItem.warningForeground": "#ffffff",
		"statusBarItem.warningBackground": "#ff6600",
		"statusBarItem.errorForeground": "#ffffff",
		"statusBarItem.errorBackground": "#ff0080",
		//active text
		"tab.activeForeground": "#ffffff",
		"tab.hoverForeground": "#f0ad31",
		"sideBarTitle.foreground": "#e0dcd0",
		"sideBarSectionHeader.foreground": "#e0dcd0",
		"panelTitle.activeForeground": "#e0dcd0",
		"statusBar.foreground": "#e0dcd0",
		//git
		"gitDecoration.untrackedResourceForeground": "#6fbd16",
		"gitDecoration.stageModifiedResourceForeground": "#1fb6f1",
		"gitDecoration.modifiedResourceForeground": "#dab759",
		"gitDecoration.conflictingResourceForeground": "#ff3300",
		//merge
		//inactive object
		"tab.inactiveForeground": "#afaba3",
		"activityBar.inactiveForeground": "#8a847d",
		"sideBar.foreground": "#c4bfb6",
		//border
		"panel.border": "#505f6d",
		"sideBarSectionHeader.border": "#424f5c",
		"sideBar.border": "#343e47",
		"activityBar.border": "#343e47",
		"statusBar.border": "#343e47",
		//scrollbar
		"scrollbarSlider.background": "#5662a875",
		"scrollbarSlider.hoverBackground": "#5662a8b6",
		"scrollbarSlider.activeBackground": "#5c56a8e1",
		//tab hover
		"tab.activeBorderTop": "#f0ad31",
		//focus
		"focusBorder": "#f0ad31",
		"list.dropBackground": "#f0ad3136",
		"editorGroup.dropBackground": "#e6cb9a36",
		//badges
		"badge.background": "#f0ad31",
		"activityBarBadge.background": "#f0ad31",
		"activityBar.foreground": "#f3efe8",
		"badge.foreground": "#000000",
		"activityBarBadge.foreground": "#000000",
		"button.background": "#f0ad31",
		"button.foreground": "#000000",
	},
	"tokenColors": [
		{
			"name": "COMMENT",
			"scope": [
				"comment",
				"punctuation.definition.comment",
			],
			"settings": {
				"foreground": "#67899e"
			}
		},
		{
			"name": "VARIABLE",
			"settings": {
				"foreground": "#dbd9cc"
			},
			"scope": [
				"variable",
				"namespace",
				"entity.name.namespace"
			]
		},
		{
			"name": "KEYWORD",
			"settings": {
				"foreground": "#fea740",
				"fontStyle": ""
			},
			"scope": [
				"keyword",
				"entity",
			]
		},
		{
			"name": "CONSTANT",
			"settings": {
				"foreground": "#c6c9df",
			},
			"scope": [
				"constant",
				"string"
			]
		},
		{
			"name": "SIMPLE CONSTANT",
			"settings": {
				"foreground": "#fea740",
				"fontStyle": ""
			},
			"scope": [
				"constant.character.escape",
				"constant.language.boolean",
				"constant.language.true",
				"constant.language.false",
			]
		},
		{
			"name": "STORAGE & IMPORT",
			"settings": {
				"foreground": "#ec4585",
				"fontStyle": ""
			},
			"scope": [
				"storage",
				// Import
				"keyword.control.import",
				"punctuation.definition.directive",
				"keyword.control.directive"
			]
		},
		{
			"name": "NORMAL CONTROL",
			"settings": {
				"foreground": "#f3cb17",
				"fontStyle": ""
			},
			"scope": [
				"keyword.control",
				"keyword.control.flow.python"
			]
		},
		{
			"name": "SPECIAL CONTROL",
			"settings": {
				"foreground": "#56a6e7",
				"fontStyle": ""
			},
			"scope": [
				"keyword.control.flow",
				"keyword.control.return",
				"keyword.control.break",
				"keyword.control.throw",
				"keyword.operator.new",
				"keyword.operator.delete"
			],
		},
		{
			"name": "ASSIGNMENT",
			"settings": {
				"foreground": "#bab0c5",
				"fontStyle": "bold"
			},
			"scope": [
				"keyword.operator.assignment"
			]
		},
		{
			"name": "OPERATOR & POINTER",
			"settings": {
				"foreground": "#eb51ff",
				"fontStyle": "bold",
			},
			"scope": [
				"keyword.operator",
				"punctuation.accessor",
				"punctuation.separator.period",
				"punctuation.separator.pointer-access",
				"punctuation.separator.dot-access",
				"keyword.operator.assignment.compound",
				// C/C++ specifics
				"storage.modifier.pointer",
				"storage.modifier.reference",
				"storage.type.function.arrow"
			]
		},
		{
			"name": "COMPARISON",
			"settings": {
				"foreground": "#f3cb17",
				"fontStyle": "bold"
			},
			"scope": [
				"keyword.operator.ternary",
				"keyword.operator.logical",
				"keyword.operator.comparison",
				"keyword.operator.bitwise",
				"keyword.operator.and",
				"keyword.operator.or",
				"keyword.operator.not",
				"keyword.operator.relational"
			]
		},
		{
			"name": "HEADING",
			"settings": {
				"foreground": "#e6e1db",
				"fontStyle": "bold"
			},
			"scope": [
				"variable.function",
				"entity.name.function",
			]
		},
		{
			"name": "LIGHT PUNCTUATION",
			"settings": {
				"foreground": "#9291aa",
				"fontStyle": ""
			},
			"scope": [
				"punctuation.section.block",
				"meta.tag",
				"constant.other.symbol",
				"constant.other.key",
				"constant.other.color",
				"punctuation.definition.tag",
				"punctuation.definition.block",
				"punctuation.definition.dictionary",
				"punctuation.separator.inheritance",
				"keyword.other.template",
				"keyword.other.substitution",
				"keyword.operator.type.annotation",
				"markup.raw.block"
			]
		},
		{
			"name": "ERROR",
			"settings": {
				"foreground": "#FF5370",
				"fontStyle": "bold underline",
			},
			"scope": [
				"invalid",
				"message.error",
				"punctuation.definition.deleted",
				"markup.deleted"
			]
		},
		{
			"name": "DARK PUNCTUATION",
			"settings": {
				"foreground": "#bab0c5",
			},
			"scope": [
				"meta.brace.round",
				"constant.character.format.placeholder",
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.section",
				"punctuation.definition.list.begin.python",
				"punctuation.definition.list.end.python",
				"meta.brace.square",
				"punctuation.definition.string"
			]
		},
		{
			"name": "FUNCTION PARENTHESIS",
			"settings": {
				"foreground": "#d6d6c6",
				"fontStyle": "bold"
			},
			"scope": [
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.section.arguments",
				"punctuation.section.parameters",
				"punctuation.definition.parameters.begin.ts",
				"punctuation.definition.parameters.end.ts"
			]
		},
		{
			"name": "MARKUP LINK",
			"settings": {
				"foreground": "#56a6e7",
				"fontStyle": ""
			},
			"scope": [
				"markup.underline.link"
			]
		},
		{
			"name": "MARKUP LINK DESCRIPTION",
			"settings": {
				"foreground": "#f3cb17",
				"fontStyle": ""
			},
			"scope": [
				"string.other.link.description",
				"string.other.link.title"
			]
		},
		{
			"name": "MARKUP BOLD",
			"settings": {
				"foreground": "#e6e1db",
				"fontStyle": "bold"
			},
			"scope": [
				"markup.bold"
			]
		},
		{
			"name": "MARKUP HEADING",
			"settings": {
				"foreground": "#ec4585",
				"fontStyle": "bold"
			},
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			]
		},
		{
			"name": "MARKUP ITALIC",
			"settings": {
				"fontStyle": "italic"
			},
			"scope": [
				"markup.italic"
			]
		},
		{
			"name": "MARKUP LIST",
			"settings": {
				"foreground": "#f3cb17",
				"fontStyle": ""
			},
			"scope": [
				"punctuation.definition.list",
			]
		},
		{
			"name": "MARKUP QUOTE",
			"settings": {
				"foreground": "#67899e",
			},
			"scope": [
				"markup.quote"
			]
		},
		{
			"name": "MARKUP RAW",
			"settings": {
				"foreground": "#67899e",
			},
			"scope": [
				"markup.raw.block"
			]
		},
		{
			"name": "MARKUP OTHER",
			"settings": {
				"foreground": "#618b31",
			},
			"scope": [
				"markup.other"
			]
		},
		{
			"name": "META SEPRATOR",
			"settings": {
				"foreground": "#9291aa",
			},
			"scope": [
				"meta.separator",
				"punctuation.definition.bold",
				"punctuation.definition.italic",
				"punctuation.definition.metadata",
				"punctuation.definition.array",
				"punctuation.support.type"
			]
		},
		{
			"name": "PROPERTY JSON",
			"settings": {
				"foreground": "#e2e0d5",
			},
			"scope": [
				"support.type.property-name"
			]
		},
		{
			"name": "LANGUAGE JSON",
			"settings": {
				"foreground": "#f3cb17",
			},
			"scope": [
				"constant.language.json"
			]
		},
		{
			"name": "TAG",
			"settings": {
				"foreground": "#ec4585",
			},
			"scope": [
				"entity.name.tag"
			]
		},
		{
			"name": "ATTRIBUTE",
			"settings": {
				"foreground": "#fea740",
			},
			"scope": [
				"entity.other.attribute-name"
			]
		}
	]
}